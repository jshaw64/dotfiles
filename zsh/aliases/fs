alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."

alias cpwd="pwd | pbcopy"
alias cdwd='cd "$(pbpaste)"'

alias rm='rm -i'
alias rmd='rmdir'
alias unl='unlink'

alias pd='popd'

alias luks_mount="$jfs_dir_scripts/luks-utils/mount.sh"
alias luks_umount="$jfs_dir_scripts/luks-utils/unmount.sh"
