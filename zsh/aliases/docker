alias bmi='docker rmi $(docker images -q)'
alias bmf='docker rm -f $(docker ps -a -q)'
alias dki='docker images'
alias dkps='docker ps -a'
alias dkrmi='docker rmi'
alias dkrmf='docker rm -f'
alias dkcb='docker-compose build'
alias dkcu='docker-compose up'
alias dkcub='docker-compose up --build'
alias dkcs='docker-compose start'
alias dkcss='docker-compose stop'
alias dkcd='docker-compose down -v'
alias dknls='docker network ls'
alias dkna='docker network add'
alias dknrm='docker network rm'
alias dkvls='docker volume ls'
alias dkvi='docker volume inspect'
alias dkvrm='docker volume rm'
alias dkvp='docker volume prune'
